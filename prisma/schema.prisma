generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TipoOperacao {
  id        Int        @id @default(autoincrement())
  nome      String     @unique
  operacoes Operacao[]
}

model Operacao {
  id             Int          @id @default(autoincrement())
  codigo         String       @unique
  nome           String       @unique
  tipoOperacao   TipoOperacao @relation(fields: [tipoOperacaoId], references: [id])
  tipoOperacaoId Int
}

model MetodoTalhao {
  id     Int    @id @default(autoincrement())
  metodo String @unique
}
